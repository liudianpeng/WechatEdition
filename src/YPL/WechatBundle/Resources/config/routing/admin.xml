    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="ypl_wechat_admin" path="/">
        <default key="_controller">YPLWechatBundle:Admin:dashboard</default>
    </route>
    <route id="ypl_wechat_admin_menu" path="/menu">
        <default key="_controller">YPLWechatBundle:Admin\Menu:index</default>
    </route>
</routes>
